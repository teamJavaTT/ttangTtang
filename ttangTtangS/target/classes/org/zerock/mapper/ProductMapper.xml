<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zerock.mapper.ProductMapper">

	<select id="selectCategory" resultType="org.zerock.domain.Category">
		select * 
		from category 
		order by maincategory
	</select>
	
	<select id="selectProduct" resultType="org.zerock.domain.Product">
		<![CDATA[
		select * 
		from product 
		where sellcheck='N' 
		order by ino desc
		]]>
	</select>
	
	

</mapper>
